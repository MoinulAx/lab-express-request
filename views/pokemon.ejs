<!-- views/pokemonList.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Pokemon</title>
</head>
<body>
    <h1>List of Pokemon</h1>
    <ul>
        <% pokemon.forEach(poke => { %>
            <li><a href="/pokemon-pretty/<%= encodeURIComponent(poke.name) %>"><%= poke.name %></a></li>
        <% }); %>
    </ul>
</body>
</html>
